Host android-tunnel
    HostName localhost
    User soto
    IdentityFile /home/soto/.ssh/id_rsa
    StrictHostKeyChecking no
    UserKnownHostsFile /dev/null

Host localhost
    User soto
    IdentityFile /home/soto/.ssh/id_rsa
    StrictHostKeyChecking no
    UserKnownHostsFile /dev/null

Host 127.0.0.1
    User soto
    IdentityFile /home/soto/.ssh/id_rsa
    StrictHostKeyChecking no
    UserKnownHostsFile /dev/null
